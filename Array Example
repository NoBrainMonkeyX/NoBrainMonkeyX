DECLARE Names : ARRAY[1:3] OF STRING
DECLARE Marks : ARRAY[1:3] OF INTEGER
DECLARE Count : INTEGER

Names[1] ← "Ahmed"
Names[2] ← "Amir"
Names[3] ← "Mona"

Marks[1]← 7
Marks[2]← 8
Marks[3]← 6


FOR count ← 1 TO 3
    OUTPUT Names[count],"  ",Marks[count]
NEXT count

//Average
DECLARE Total :INTEGER
Total ← 0
FOR count ← 1 To 3
    Total ← Total + Marks[count]
NEXT count
OUTPUT "class average", Total / 3

DECLARE Highest : INTEGER
Highest ← 0
FOR count ← 1 TO 3
    IF Marks[count] > Highest THEN
        Highest ← Marks[count]
    ENDIF
NEXT count
